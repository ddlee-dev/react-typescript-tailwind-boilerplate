{
  "name": "react-typescript-boilerplate",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --coverage",
    "analyze": "craco build && webpack-bundle-analyzer",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "eslint": "./node_modules/.bin/eslint --cache --cache-location=node_modules/.cache/.eslintcache --fix",
    "stylelint": "./node_modules/.bin/stylelint --cache --cache-location=node_modules/.cache/.stylelintcache --fix",
    "lint:js": "npm run eslint 'src/**/*.{ts,tsx}'",
    "lint:css": "npm run stylelint 'src/**/*.css'",
    "copy:env": "cp .env.example .env"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": "npm run lint:js",
    "src/**/*.css": "npm run lint:css",
    "src/**/*.{js,ts,tsx,json,css,md}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "@xstate/react": "^1.3.3",
    "axios": "^0.21.1",
    "nanoid": "^3.1.23",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^1.0.2",
    "react-modal": "^3.13.1",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "xml2js": "^0.4.23",
    "xstate": "^4.19.1"
  },
  "devDependencies": {
    "@craco/craco": "^6.1.2",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-modal": "^3.12.0",
    "@types/xml2js": "^0.4.8",
    "autoprefixer": "^9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^13.0.0",
    "postcss": "^7",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "web-vitals": "^1.1.1",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
